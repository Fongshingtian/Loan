package com.ht.bean;

import lombok.Data;
import lombok.NoArgsConstructor;
import lombok.experimental.Accessors;

/**
 * 成长任务表
 */

@NoArgsConstructor
@Data
@Accessors(chain=true)
public class Task {
    private Integer id;//主键自增
    private Integer userId;//用户id
    private Integer bindPhone;//绑定手机
    private Integer authentication;//实名认证
    private Integer bindBankCard;//绑定银行卡
    private Integer firstRecharge1h;//首次充值一百以上
    private Integer firstBid;//第一次投标
    private Integer firstBid3t;//第一次投标大于三千元
    private Integer firstMonthBid5t;//每月第一次投标大于五千
    private Integer firstMonthBid10t;//每月第一次投标大于一万
    private Integer firstMonthBid30t;//每月第一次投标大于三万
    private Integer firstMonthBid50t;//每月第一次投标大于五万
}
