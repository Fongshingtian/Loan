import lombok.Data;
import lombok.NoArgsConstructor;
import lombok.experimental.Accessors;

/**
 * 优惠表与用户表关联的中间表（middlecount）（增删改查）
 * 	mid（id）
 * 	uid（用户id）
 * 	did（红包id）
 * 	mstate状态（1未兑现，2已使用，3已过期）
 */
@Data
@NoArgsConstructor
@Accessors(chain = true
)
public class middlecount {
    private Integer mid;
    private Integer uid;
    private Integer did;
    private String mstate;
}
